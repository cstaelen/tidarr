{
  "name": "tidarr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "front-build": "next build",
    "front-lint": "next lint",
    "api-build": "npx tsc --project api/tsconfig.json",
    "dev": "concurrently -n api,api,front \"npx tsc --project api/tsconfig.json --watch\" \"nodemon -q api/dist/index.js\" \"next dev -p 8484\"",
    "prod": "concurrently -n front,api \"PORT=8484 node ./server.js\" \"node ./api/dist/index.js\""
  },
  "devDependencies": {
    "@types/express": "^4.17.18",
    "@types/node": "^20.6.0",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/react-swipeable-views": "^0.13.2",
    "concurrently": "^8.2.1",
    "eslint": "^8.49.0",
    "eslint-config-next": "13.4.9",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "autoprefixer": "10.4.14",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "next": "13.4.9",
    "next-pwa": "^5.6.0",
    "postcss": "8.4.25",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-swipeable-views": "^0.14.0",
    "sharp": "^0.32.5",
    "webpack": "^5.88.2"
  }
