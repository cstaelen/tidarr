    "@mui/icons-material": "^5.15.1",
    "@mui/material": "^5.15.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^17.0.45",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.2",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "^5.0.1",
    "ts-prune": "^0.10.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "serve": "^14.2.3",
    "sharp": "^0.32.5"
  },
  "scripts": {
    "front-build": "react-scripts build",
    "api-build": "npx tsc --project api/tsconfig.json",
    "dev": "concurrently -n api,api,front \"npx tsc --project api/tsconfig.json --watch\" \"nodemon -q api/dist/index.js\" \"PORT=8484 yarn start\"",
    "prod": "node ./api/dist/index.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "find-deadcode": "ts-prune | (! grep -v 'used in module')",
    "depcheck": "npx depcheck",
    "eslint": "eslint .",
    "eslint-fix": "eslint --fix ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}