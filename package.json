{
  "name": "tidarr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "front-build": "next build --debug",
    "front-lint": "next lint",
    "api-build": "npx tsc --project api/tsconfig.json",
    "dev": "concurrently -n api,api,front \"npx tsc --project api/tsconfig.json --watch\" \"nodemon -q api/dist/index.js\" \"next dev -p 8484\"",
    "prod": "concurrently -n front,api \"PORT=8484 node ./server.js\" \"node ./api/dist/index.js\""
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "nodemon": "^3.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.1",
    "@mui/material": "^5.15.1",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "next": "^14.0.4",
    "next-pwa": "^5.6.0",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.32.5",
    "webpack": "^5.89.0"
  }

}
