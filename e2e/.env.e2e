# E2E Test Environment Variables
# These variables are loaded into Docker containers during E2E tests

# Enable test mode
CI=true

# Disable features that require external services
ENABLE_BEETS=false
ENABLE_TIDAL_PROXY=true

# Plex (optional - use mock URLs for tests)
PLEX_URL=http://plex.url
PLEX_TOKEN=plex-token
PLEX_LIBRARY=3

# Gotify (disabled for tests)
GOTIFY_URL=
GOTIFY_TOKEN=

# Navidrome (disabled for tests)
NAVIDROME_URL=
NAVIDROME_USER=
NAVIDROME_PASSWORD=

# Apprise (disabled for tests)
APPRISE_API_ENDPOINT=
APPRISE_API_TAG=

# Admin password (optional)
ADMIN_PASSWORD=

# Quality settings (optional)
LOCK_QUALITY=
VERSION=0.0.0-testing

# Active download history
ENABLE_HISTORY=true

# User/Group IDs (use host user for permissions)
PUID=1000
PGID=1000
UMASK=0002